{
	"name": "ingestion-lambda",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"dev": "INGESTION_LAMBDA_QUEUE_URL=http://sqs.eu-west-1.localhost.localstack.cloud:4566/000000000000/local-ingestion-queue tsx watch localRun.ts",
		"db-refresh": "tsx watch randomisedDatabaseInserts.ts",
